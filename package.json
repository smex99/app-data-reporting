		"concurrently": "^4.1.0",
		"cors": "^2.8.5",
		"cross-env": "^5.2.0",
		"express": "^4.17.3",
		"express-promise-router": "^3.0.3",
		"mongodb": "^3.3.2",
		"mongoose": "^5.4.20",
		"morgan": "^1.9.1"
	},
	"devDependencies": {
		"nodemon": "^1.18.10"
	}
}
